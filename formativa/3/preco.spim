.data 
    c100: .double 100.0

.text
main:
    li $v0, 7 
    syscall 
    mov.d $f2, $f0  # f2 = preco

    syscall 
    mov.d $f4, $f0 # f4 = % lucro
    

    syscall
    add.d $f4, $f4, $f0 # f4 = % lucro + % impostos

    l.d $f6, c100 # f6 = 100

    div.d $f4, $f4, $f6 # f4 = % total / 100
    mul.d $f4, $f4, $f2 # f4 = % total / 100 * preco
    add.d $f12, $f2, $f4 # f12 = preco + preco * % total

    li $v0, 3
    syscall 

    li $v0, 10
    syscall 